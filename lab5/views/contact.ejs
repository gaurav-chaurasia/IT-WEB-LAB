<%- include('./partials/header') %>
<p class="center display-4">contact</p>

<div class="container center-xy my-3">
    <form action="/users/contact" method="post">
        <div class="form-group row">
            <div class="cl-md-6">
                <label for="firstname">firstname</label>
                <input type="text" name="firstname" class="form-control">
            </div>
            <div class="cl-md-6 ml-1">
                <label for="fir">lastname</label>
                <input type="text" name="lastname" class="form-control">
            </div>
        </div>
        <div class="form-group row">
            <label for="email">email</label>
            <input type="email" name="email" class="form-control">
        </div>
        <div class="form-group row">
            <label for="dob">Date of Birth</label>
            <input type="datetime-local" name="dob" class="form-control">
        </div>
        <div class="form-group row">
            <label for="query">query</label>
            <textarea type="text" name="query" class="form-control"></textarea>
        </div>
        <button type="submit" class="row btn btn-dark">submit</button>
    </form>
</div>
<%- include('./partials/footer') %>
